# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# LINE Login Channel (for LIFF)
# Create a channel in LINE Developers console with "LINE Login" provider
NEXT_PUBLIC_LIFF_ID=your_liff_id_here

# LINE Messaging API Channel (for Push Notifications/Flex Messages)
# Create a channel in LINE Developers console with "Messaging API" provider
# Issue a long-lived channel access token in the Messaging API settings
LINE_CHANNEL_ACCESS_TOKEN=your_channel_access_token_here
